{
  "name": "vue-service-bay",
  "version": "0.0.1",
  "description": "Small utility to modify Vue components in bulk",
  "main": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsc -d -w",
    "build": "tsc -d",
    "type:check": "tsc --noEmit",
    "format:check": "prettier --cache --check \"./src/**/*.{js,ts,vue,json}\"",
    "format:fix": "prettier --cache --write \"./src/**/*.{js,ts,vue,json}\"",
    "publish:check": "publint",
    "test:watch": "vitest",
    "check:all": "pnpm type:check && pnpm format:check && pnpm publish:check"
  },
  "keywords": [
    "vue",
    "vue3",
    "ast",
    "static analysis",
    "code generation",
    "code refactor"
  ],
  "author": "baseballyama",
  "license": "MIT",
  "devDependencies": {
    "publint": "0.2.5",
    "vitest": "0.34.6"
  },
  "dependencies": {
    "@babel/types": "7.23.0",
    "dom-serializer": "2.0.0",
    "domhandler": "5.0.3",
    "globby": "13.2.2",
    "htmlparser2": "9.0.0",
    "magic-string": "0.30.5",
    "postcss": "8.4.31",
    "postcss-scss": "4.0.9",
    "recast": "0.23.4",
    "zimmerframe": "1.0.0"
  }
}
